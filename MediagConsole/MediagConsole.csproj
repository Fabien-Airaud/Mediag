    <AssemblyTitle>MediagConsole</AssemblyTitle>
    <Company>HP</Company>
    <Product>MediagConsole</Product>
    <Copyright>Copyright © HP 2024</Copyright>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
